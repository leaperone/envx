{
  "name": "envx",
  "version": "1.0.0",
  "description": "A powerful environment management CLI tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "bin": {
    "envx": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
